{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "strict": false,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "baseUrl": ".",
    "declaration": true,
    "jsxImportSource": "@emotion/react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "target": "es6",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  }
}
