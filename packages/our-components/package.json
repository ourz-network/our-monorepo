    "react": "latest",
    "react-dom": "latest"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@ourz/odk": "latest",
    "@types/react": "^18.0.25",
    "@zoralabs/nft-hooks": "latest",
    "degene-sais-quoi": "latest",
    "merge-anything": "^5.0.4",
