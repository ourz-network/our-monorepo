{
  "name": "@ourz/our-hooks",
  "version": "1.0.2",
  "description": "Generic react hooks for fetching ourz.network Splits, NFTs, and other misc data. Fork of @zoralabs/nft-hooks.",
  "author": "ourz-network",
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc",
    "prepublish": "yarn build",
    "gen-graphql-types": "graphql-code-generator",
    "load-schemas": "./graph-schemas/get-schemas.sh"
  },
  "dependencies": {
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "big.js": "^6.1.1",
    "cross-fetch": "^3.1.5",
    "dataloader": "^2.0.0",
    "graphql": "^16.3.0",
    "graphql-request": "^3.7.0",
    "node-abort-controller": "^2.0.0",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "sha3": "^2.1.4",
    "swr": "^0.5.6",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-tools/mock": "^8.5.1",
    "@graphql-tools/schema": "^8.3.1",
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/big.js": "^6.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "fetch-mock-jest": "^1.5.1",
    "graphql-tools": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-serial-runner": "^1.2.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md}": "prettier src --write"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ourz-network/our-hooks"
  },
  "bugs": {
    "url": "https://github.com/ourz-network/our-hooks/issues"
  },
  "homepage": "https://github.com/ourz-network/our-hooks#README"
}
